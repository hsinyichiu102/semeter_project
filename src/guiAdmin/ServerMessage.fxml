<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="500.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.NewsListController">
   <children>
      <Button fx:id="mainViewButton" mnemonicParsing="false" onAction="#mainViewButtonPushed" text="Back to Main View" StackPane.alignment="BOTTOM_RIGHT">
         <font>
            <Font size="20.0" />
         </font>
         <StackPane.margin>
            <Insets bottom="5.0" right="5.0" />
         </StackPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#detailedNewsButtonPushed" text="Add news" StackPane.alignment="BOTTOM_CENTER">
         <StackPane.margin>
            <Insets bottom="5.0" />
         </StackPane.margin>
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TableView fx:id="tableView" maxHeight="350.0" maxWidth="740.0" prefHeight="350.0" prefWidth="740.0" StackPane.alignment="TOP_CENTER">
        <columns>
          <TableColumn fx:id="headlineColumn" prefWidth="95.80575156211853" text="Message ID" />
          <TableColumn fx:id="publisherColumn" minWidth="0.0" prefWidth="234.0" text="News URL" />
            <TableColumn fx:id="urlColumn" prefWidth="411.0" text="Message" />
        </columns>
      </TableView>
   </children>
</StackPane>
