<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AddNewsController">
   <children>
      <StackPane prefHeight="500.0" prefWidth="740.0" StackPane.alignment="CENTER">
         <children>
            <Label text="Added!" StackPane.alignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
               <StackPane.margin>
                  <Insets bottom="100.0" />
               </StackPane.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#mainViewButtonPushed" text="Back to Main View">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </StackPane>
   </children>
</StackPane>
